<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>digiturnos.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>digiturnos.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>digiturnos.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmLogin</managed-bean-name>
        <managed-bean-class>digiturnos.frmLogin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmPrincipal</managed-bean-name>
        <managed-bean-class>digiturnos.frmPrincipal</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/frmLogin.jsp</from-view-id>
        <navigation-case>
            <from-outcome>logueoExitoso</from-outcome>
            <to-view-id>/frmPrincipal.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>frmReservarTurno</managed-bean-name>
        <managed-bean-class>digiturnos.frmReservarTurno</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmVerTurnos</managed-bean-name>
        <managed-bean-class>digiturnos.frmVerTurnos</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/frmPrincipal.jsp</from-view-id>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmLogin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>frmServicios</managed-bean-name>
        <managed-bean-class>digiturnos.frmServicios</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmServicio</managed-bean-name>
        <managed-bean-class>digiturnos.frmServicio</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/frmServicios.jsp</from-view-id>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmLogin.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevoServicio</from-outcome>
            <to-view-id>/frmServicio.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editarServicio</from-outcome>
            <to-view-id>/frmServicio.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>frmVerAgenda</managed-bean-name>
        <managed-bean-class>digiturnos.frmVerAgenda</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmOtorgarTurnos</managed-bean-name>
        <managed-bean-class>digiturnos.frmOtorgarTurnos</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmPacientes</managed-bean-name>
        <managed-bean-class>digiturnos.frmPacientes</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmEspecialidades</managed-bean-name>
        <managed-bean-class>digiturnos.frmEspecialidades</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmProfesionales</managed-bean-name>
        <managed-bean-class>digiturnos.frmProfesionales</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmBandasHorarias</managed-bean-name>
        <managed-bean-class>digiturnos.frmBandasHorarias</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmOtorgarLicencias</managed-bean-name>
        <managed-bean-class>digiturnos.frmOtorgarLicencias</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmUsuarios</managed-bean-name>
        <managed-bean-class>digiturnos.frmUsuarios</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmHorarios</managed-bean-name>
        <managed-bean-class>digiturnos.frmHorarios</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmEspecialidad</managed-bean-name>
        <managed-bean-class>digiturnos.frmEspecialidad</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmPaciente</managed-bean-name>
        <managed-bean-class>digiturnos.frmPaciente</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmProfesional</managed-bean-name>
        <managed-bean-class>digiturnos.frmProfesional</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>frmUsuario</managed-bean-name>
        <managed-bean-class>digiturnos.frmUsuario</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/frmServicio.jsp</from-view-id>
        <navigation-case>
            <from-outcome>Aceptar</from-outcome>
            <to-view-id>/frmServicios.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Cancelar</from-outcome>
            <to-view-id>/frmServicios.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmServicios.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmLogin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/frmEspecialidades.jsp</from-view-id>
        <navigation-case>
            <from-outcome>editarEspecialidad</from-outcome>
            <to-view-id>/frmEspecialidad.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevaEspecialidad</from-outcome>
            <to-view-id>/frmEspecialidad.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmLogin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/frmEspecialidad.jsp</from-view-id>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmLogin.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Aceptar</from-outcome>
            <to-view-id>/frmEspecialidades.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Cancelar</from-outcome>
            <to-view-id>/frmEspecialidades.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/frmPacientes.jsp</from-view-id>
        <navigation-case>
            <from-outcome>editarPaciente</from-outcome>
            <to-view-id>/frmPaciente.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevoPaciente</from-outcome>
            <to-view-id>/frmPaciente.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmLogin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/frmPaciente.jsp</from-view-id>
        <navigation-case>
            <from-outcome>Cancelar</from-outcome>
            <to-view-id>/frmPacientes.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmLogin.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Aceptar</from-outcome>
            <to-view-id>/frmPacientes.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/frmProfesionales.jsp</from-view-id>
        <navigation-case>
            <from-outcome>editarHorarios</from-outcome>
            <to-view-id>/frmHorarios.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editarProfesional</from-outcome>
            <to-view-id>/frmProfesional.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/frmProfesional.jsp</from-view-id>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmLogin.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancelar</from-outcome>
            <to-view-id>/frmProfesionales.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>aceptar</from-outcome>
            <to-view-id>/frmProfesionales.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/frmHorarios.jsp</from-view-id>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmLogin.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/frmProfesionales.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/frmVerTurnos.jsp</from-view-id>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmLogin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>frmVerTurnosHistoricos</managed-bean-name>
        <managed-bean-class>digiturnos.frmVerTurnosHistoricos</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/frmVerTurnosHistoricos.jsp</from-view-id>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmLogin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/frmOtorgarTurnos.jsp</from-view-id>
        <navigation-case>
            <from-outcome>cancelar</from-outcome>
            <to-view-id>/frmPrincipal.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/frmLogin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
